         // Testing Functions for Data Integrity and Performance
         function runDataIntegrityTests() {
             debugLog('=== Iniciando Testes de Integridade dos Dados ===');
             
             const testResults = {
                 totalTests: 0,
                 passedTests: 0,
                 failedTests: 0,
                 errors: []
             };
             
             // Test 1: Check for duplicate codes
             testResults.totalTests++;
             const codes = allIncidents.map(incident => incident.codigo);
             const uniqueCodes = [...new Set(codes)];
             if (codes.length === uniqueCodes.length) {
                 testResults.passedTests++;
                 debugLog('‚úì Teste 1 PASSOU: N√£o h√° c√≥digos duplicados');
             } else {
                 testResults.failedTests++;
                 testResults.errors.push('C√≥digos duplicados encontrados');
                 debugLog('‚úó Teste 1 FALHOU: C√≥digos duplicados encontrados');
             }
             
             // Test 2: Check required fields
             testResults.totalTests++;
             const requiredFields = ['codigo', 'data', 'status'];
             let missingFieldsCount = 0;
             allIncidents.forEach((incident, index) => {
                 requiredFields.forEach(field => {
                     if (!incident[field] || incident[field].toString().trim() === '') {
                         missingFieldsCount++;
                     }
                 });
             });
             
             if (missingFieldsCount === 0) {
                 testResults.passedTests++;
                 debugLog('‚úì Teste 2 PASSOU: Todos os campos obrigat√≥rios est√£o preenchidos');
                } else {
                    testResults.failedTests++;
                    testResults.errors.push(`${missingFieldsCount} campos obrigat√≥rios ausentes`);
                    debugLog(`‚úó Teste 2 FALHOU: ${missingFieldsCount} campos obrigat√≥rios ausentes`);
             }
             
             // Test 3: Date format validation
             testResults.totalTests++;
             let invalidDatesCount = 0;
             allIncidents.forEach(incident => {
                 if (incident.data && !isValidDate(incident.data)) {
                     invalidDatesCount++;
                 }
             });
             
             if (invalidDatesCount === 0) {
                 testResults.passedTests++;
                 debugLog('‚úì Teste 3 PASSOU: Todas as datas est√£o em formato v√°lido');
                } else {
                    testResults.failedTests++;
                    testResults.errors.push(`${invalidDatesCount} datas em formato inv√°lido`);
                    debugLog(`‚úó Teste 3 FALHOU: ${invalidDatesCount} datas em formato inv√°lido`);
             }
             
             // Test 4: Status values validation
             testResults.totalTests++;
             const validStatuses = ['Conclu√≠do', 'Em Andamento', 'Pendente', 'Cancelado'];
             let invalidStatusCount = 0;
             allIncidents.forEach(incident => {
                 if (incident.status && !validStatuses.includes(incident.status)) {
                     invalidStatusCount++;
                 }
             });
             
             if (invalidStatusCount === 0) {
                 testResults.passedTests++;
                 debugLog('‚úì Teste 4 PASSOU: Todos os status s√£o v√°lidos');
                } else {
                    testResults.failedTests++;
                    testResults.errors.push(`${invalidStatusCount} status inv√°lidos`);
                    debugLog(`‚úó Teste 4 FALHOU: ${invalidStatusCount} status inv√°lidos`);
                }
                
                debugLog(`=== Resultado dos Testes de Integridade ===`);
                debugLog(`Total: ${testResults.totalTests} | Passou: ${testResults.passedTests} | Falhou: ${testResults.failedTests}`);
             
             return testResults;
         }
         
