        // Tab system functionality
        function initializeTabs() {
            const tabBtns = document.querySelectorAll('.tab-btn');
            const tabContents = document.querySelectorAll('.tab-content');
            
            tabBtns.forEach(btn => {
                btn.addEventListener('click', function() {
                    const tabId = this.dataset.tab;
                    
                    // Remove active class from all tabs and contents
                    tabBtns.forEach(b => b.classList.remove('active'));
                    tabContents.forEach(content => content.classList.remove('active'));
                    
                    // Add active class to clicked tab
                    this.classList.add('active');
                    
                    // Show corresponding content
                    const targetContent = tabId === 'dashboard' ? 
                        document.getElementById('dashboardContent') : 
                        document.getElementById('dataContent');
                    targetContent.classList.add('active');
                    
                    // Initialize table if switching to data tab
                    if (tabId === 'data') {
                        updateTable();
                    }
                });
            });
        }
        
        // Data table functionality
        let currentSort = { column: null, direction: 'asc' };
        let currentPage = 1;
        let rowsPerPage = 25;
        let filteredData = [];
        let searchTerm = '';
        
        function initializeDataTable() {
            const searchInput = document.getElementById('tableSearch');
            const rowsSelect = document.getElementById('rowsPerPage');
            const exportBtn = document.getElementById('exportBtn');
            const importBtn = document.getElementById('importBtn');
            const importFile = document.getElementById('importExcelInput');
            const prevBtn = document.getElementById('prevPage');
            const nextBtn = document.getElementById('nextPage');
            const tableHeaders = document.querySelectorAll('th[data-sort]');
            
            // Search functionality
            searchInput.addEventListener('input', function() {
                searchTerm = this.value.toLowerCase();
                currentPage = 1;
                updateTable();
            });
            
            // Rows per page
            rowsSelect.addEventListener('change', function() {
                rowsPerPage = parseInt(this.value);
                currentPage = 1;
                updateTable();
            });
            
            // Export and Import functionality
            exportBtn.addEventListener('click', showExportModal);
            
            // Import functionality
            if (importBtn && importFile) {
                importBtn.addEventListener('click', function() {
                    importFile.click();
                });
                
                importFile.addEventListener('change', function(e) {
                    const file = e.target.files[0];
                    if (file) {
                        importFromExcel(file);
                        // Reset file input
                        e.target.value = '';
                    }
                });
            }
            
            // Modal functionality
            const closeModal = document.getElementById('closeModal');
            const cancelExport = document.getElementById('cancelExport');
            const confirmExport = document.getElementById('confirmExport');
            const exportModal = document.getElementById('exportModal');
            
            if (closeModal) closeModal.addEventListener('click', hideExportModal);
            if (cancelExport) cancelExport.addEventListener('click', hideExportModal);
            if (confirmExport) confirmExport.addEventListener('click', exportToExcel);
            
            // Close modal when clicking outside
            if (exportModal) {
                exportModal.addEventListener('click', function(e) {
                    if (e.target === exportModal) {
                        hideExportModal();
                    }
                });
            }
            
            // Pagination
            prevBtn.addEventListener('click', function() {
                if (currentPage > 1) {
                    currentPage--;
                    updateTable();
                }
            });
            
            nextBtn.addEventListener('click', function() {
                const totalPages = Math.ceil(filteredData.length / rowsPerPage);
                if (currentPage < totalPages) {
                    currentPage++;
                    updateTable();
                }
            });
            
            // Sorting
            tableHeaders.forEach(header => {
                header.addEventListener('click', function() {
                    const column = this.dataset.sort;
                    
                    if (currentSort.column === column) {
                        currentSort.direction = currentSort.direction === 'asc' ? 'desc' : 'asc';
                    } else {
                        currentSort.column = column;
                        currentSort.direction = 'asc';
                    }
                    
                    // Update header visuals
                    tableHeaders.forEach(h => h.classList.remove('sort-asc', 'sort-desc'));
                    this.classList.add('sort-' + currentSort.direction);
                    
                    currentPage = 1;
                    updateTable();
                });
            });
            
            // Initial table population
            updateTable();
        }
        
        function updateTable() {
            // Filter incidents based on current filters and search
            filteredData = allIncidents.filter(incident => {
                // Apply existing filters
                if (filterState.status !== 'Todos' && incident.status !== filterState.status) {
                    return false;
                }
                
                if (filterState.unidade !== 'Todas' && incident.notificada !== filterState.unidade) {
                    return false;
                }
                
                if (filterState.severidade !== 'Todas' && incident.severidade !== filterState.severidade) {
                    return false;
                }
                
                if (filterState.periodo.length > 0 && !filterState.periodo.includes(incident.mes_ano)) {
                    return false;
                }
                
                // Apply search filter
                if (searchTerm) {
                    const searchString = Object.values(incident).join(' ').toLowerCase();
                    if (!searchString.includes(searchTerm)) {
                        return false;
                    }
                }
                
                return true;
            });
            
            // Sort data
            if (currentSort.column) {
                filteredData.sort((a, b) => {
                    let aVal = a[currentSort.column] || '';
                    let bVal = b[currentSort.column] || '';
                    
                    // Handle numbers
                    if (currentSort.column === 'codigo') {
                        aVal = parseInt(aVal);
                        bVal = parseInt(bVal);
                    }
                    
                    if (aVal < bVal) return currentSort.direction === 'asc' ? -1 : 1;
                    if (aVal > bVal) return currentSort.direction === 'asc' ? 1 : -1;
                    return 0;
                });
            }
            
            // Update table summary
            const totalFiltered = filteredData.length;
            const total = allIncidents.length;
            const summary = totalFiltered === total ? 
                `Exibindo ${totalFiltered} de ${total} registros` : 
                `Exibindo ${totalFiltered} de ${total} registros (filtrado)`;
            document.getElementById('tableSummary').textContent = summary;
            
            // Pagination
            const totalPages = rowsPerPage === -1 ? 1 : Math.ceil(totalFiltered / rowsPerPage);
            const startRow = rowsPerPage === -1 ? 1 : (currentPage - 1) * rowsPerPage + 1;
            const endRow = rowsPerPage === -1 ? totalFiltered : Math.min(currentPage * rowsPerPage, totalFiltered);
            
            // Update pagination info
            document.getElementById('paginationInfo').textContent = 
                `Mostrando ${startRow} at√© ${endRow} de ${totalFiltered} registros`;
            
            // Update pagination buttons
            document.getElementById('prevPage').disabled = currentPage <= 1;
            document.getElementById('nextPage').disabled = currentPage >= totalPages;
            
            // Generate page numbers
            generatePageNumbers(totalPages);
            
            // Render table rows
            renderTableRows();
        }
        
        function renderTableRows() {
            const tbody = document.getElementById('tableBody');
            const startIdx = rowsPerPage === -1 ? 0 : (currentPage - 1) * rowsPerPage;
            const endIdx = rowsPerPage === -1 ? filteredData.length : startIdx + rowsPerPage;
            const pageData = filteredData.slice(startIdx, endIdx);
            
            tbody.innerHTML = pageData.map(incident => {
                return `
                    <tr>
                        <td>${incident.codigo}</td>
                        <td>${highlightSearch(incident.ocorrencia)}</td>
                        <td>${highlightSearch(incident.data)}</td>
                        <td>${highlightSearch(incident.notificadora)}</td>
                        <td>${highlightSearch(incident.notificada)}</td>
                        <td>${highlightSearch(incident.origem)}</td>
                        <td>Incidente envolvendo o Paciente</td>
                        <td>${highlightSearch(incident.severidade)}</td>
                        <td>${highlightSearch(incident.tipo_incidente_oms)}</td>
                        <td><span class="status-badge ${incident.status.toLowerCase().replace(' ', '-').replace('√≠', 'i')}">${incident.status}</span></td>
                        <td>${highlightSearch(incident.acao_imediata)}</td>
                        <td>${highlightSearch(incident.responsavel)}</td>
                        <td><span class="fase-badge ${getFaseClass(incident.fase)}">${incident.fase}</span></td>
                        <td style="font-size: 11px;">${highlightSearch(incident.processo)}</td>
                    </tr>
                `;
            }).join('');
        }
        
        function highlightSearch(text) {
            if (!searchTerm || !text) return text;
            const regex = new RegExp(`(${searchTerm})`, 'gi');
            return text.toString().replace(regex, '<span class="search-highlight">$1</span>');
        }
        
        function getFaseClass(fase) {
            switch(fase) {
