        function updateResponsaveis(filteredIncidents) {
            const responsaveisContainer = document.querySelector('.responsaveis-list');
            if (!responsaveisContainer) return;
            
            // Count responsaveis from filtered data
            const responsaveisCount = {};
            filteredIncidents.forEach(incident => {
                responsaveisCount[incident.responsavel] = (responsaveisCount[incident.responsavel] || 0) + 1;
            });
            
            // Sort by count and take top 6
            const sortedResponsaveis = Object.entries(responsaveisCount)
                .sort(([,a], [,b]) => b - a)
                .slice(0, 6);
            
            // Generate HTML
            responsaveisContainer.innerHTML = sortedResponsaveis.map(([name, count]) => {
                const initials = name.split('.').map(n => n.charAt(0).toUpperCase()).join('');
                return `
                    <div class="responsavel-item">
                        <div class="responsavel-info">
                            <div class="avatar">${initials}</div>
                            <div class="responsavel-name">${name}</div>
                        </div>
                        <div class="responsavel-count">${count}</div>
                    </div>
                `;
            }).join('');
        }

